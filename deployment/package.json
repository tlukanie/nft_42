    "node": "npx hardhat node",
    "clean": "npx hardhat clean"
  },
  "keywords": [
    "nft",
    "erc-721",
    "ethereum",
    "sepolia",
    "solidity",
    "blockchain",
    "parrodessa42"
  ],
