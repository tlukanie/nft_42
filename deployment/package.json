    "@nomicfoundation/hardhat-verify": "^1.1.0",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^10.0.1",
    "chai": "^4.3.0",
    "ethers": "^6.7.1",
    "hardhat": "^2.17.1",
